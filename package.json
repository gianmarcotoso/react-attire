{
	"name": "react-attire",
	"version": "0.4.3",
	"description": "A dress code for your React forms",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "https://github.com/gianmarcotoso/react-attire",
	"author": "Gian Marco Toso <info@gianmarcotoso.com>",
	"license": "MIT",
	"private": false,
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json"
		],
		"testMatch": [
			"<rootDir>/src/**/*.spec.(ts|tsx|js)"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
		},
		"setupFiles": [
			"<rootDir>/jestSetup.js"
		]
	},
	"scripts": {
		"build": "NODE_ENV=production webpack -p --progress --colors",
		"test": "jest"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@types/enzyme": "^3.1.14",
		"@types/jest": "^23.3.9",
		"@types/prop-types": "^15.5.6",
		"@types/react": "^16.4.18",
		"babel-loader": "^8.0.4",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.6.0",
		"jest": "^23.6.0",
		"jsdom": "^13.0.0",
		"react": "^16.6.0",
		"react-dom": "^16.6.0",
		"react-test-renderer": "^16.6.0",
		"ts-loader": "^5.3.0",
		"typescript": "^3.1.6",
		"webpack": "^4.23.1",
		"webpack-cli": "^3.1.2"
	}
}
