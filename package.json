{
	"name": "react-attire",
	"version": "0.1.2",
	"description": "A dress code for your React forms",
	"main": "dist/index.js",
	"repository": "https://github.com/gianmarcotoso/react-attire",
	"author": "Gian Marco Toso <info@gianmarcotoso.com>",
	"license": "MIT",
	"private": false,
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json"
		],
		"testMatch": [
			"<rootDir>/src/**/*.spec.(ts|tsx|js)"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
		},
		"setupFiles": [
			"<rootDir>/jestSetup.js"
		]
	},
	"scripts": {
		"build": "NODE_ENV=production webpack -p --progress --colors",
		"test": "jest"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/enzyme": "^3.1.3",
		"@types/jest": "^21.1.5",
		"@types/prop-types": "^15.5.2",
		"@types/react": "^16.0.21",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"enzyme": "^3.1.0",
		"enzyme-adapter-react-16": "^1.0.2",
		"jest": "^21.3.0-beta.8",
		"jsdom": "^11.3.0",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-test-renderer": "^16.0.0",
		"ts-loader": "^3.1.1",
		"typescript": "^2.6.1",
		"webpack": "^3.6.0"
	}
}
